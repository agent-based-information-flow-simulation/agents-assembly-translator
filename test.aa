agent test
    prm social_media_addiction, float, dist_normal, 0, 30
    prm susceptability, float, init, 30.5
    prm is_online, enum, yes, 80, no, 20
    prm is_dead, enum, yes, 0, no_way, 90, maybe, 10
    setupbehav run_on_start
        action check_addiction
            decl big, 5
            igt social_media_addiction, big 
                add susceptability, 15
            eblock
            decl i, 1
            wlte i, 10
                set is_dead, maybe 
                ine is_online, no_way
                    decl factorial, 1
                    decl factor, 2
                    wlt factorial, 1000
                        mult factorial, factor
                        add factor, 1
                    eblock
                eblock
                add i, 1
            eblock
            set is_dead, no_way
        eaction
    ebehav
eagent
